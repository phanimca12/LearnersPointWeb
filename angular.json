    "@awd-ng-lib/awd-job-api-client": {
      "projectType": "library",
      "root": "projects/awd-ng-lib/awd-job-api-client",
      "sourceRoot": "projects/awd-ng-lib/awd-job-api-client/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/awd-ng-lib/awd-job-api-client/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/awd-ng-lib/awd-job-api-client/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/awd-ng-lib/awd-job-api-client/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/awd-ng-lib/awd-job-api-client/src/test.ts",
            "tsConfig": "projects/awd-ng-lib/awd-job-api-client/tsconfig.spec.json",
            "karmaConfig": "projects/awd-ng-lib/awd-job-api-client/karma.conf.js"
          }
        }
      }
    },
    "learners-point-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/learners-point-app",
      "sourceRoot": "projects/learners-point-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/learners-point-app",
            "index": "projects/learners-point-app/src/index.html",
            "main": "projects/learners-point-app/src/main.ts",
            "polyfills": "projects/learners-point-app/src/polyfills.ts",
            "tsConfig": "projects/learners-point-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/learners-point-app/src/favicon.ico",
              "projects/learners-point-app/src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/primeng/resources/themes/bootstrap4-light-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeflex/primeflex.css",
              "node_modules/primeicons/primeicons.css",
              "projects/learners-point-app/src/styles.scss"
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/jquery/dist/jquery.min.js"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "projects/learners-point-app/src/scss/"
              ]
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/learners-point-app/src/environments/environment.ts",
                  "with": "projects/learners-point-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": {
                "scripts": true,
                "fonts": {
                  "inline": true
                },
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                }
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "8kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "learners-point-app:build:production"
            },
            "development": {
              "browserTarget": "learners-point-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "learners-point-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/learners-point-app/src/test.ts",
            "polyfills": "projects/learners-point-app/src/polyfills.ts",
            "tsConfig": "projects/learners-point-app/tsconfig.spec.json",
            "karmaConfig": "projects/learners-point-app/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/learners-point-app/src/favicon.ico",
              "projects/learners-point-app/src/assets"
            ],
            "styles": [
              "projects/learners-point-app/src/styles.scss"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "projects/learners-point-app/src/scss/"
              ]
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/learners-point-app/tsconfig.app.json",
              "projects/learners-point-app/tsconfig.spec.json",
              "projects/learners-point-app/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "learners-point-app"
}