{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@awd-ng-lib/session": {
      "projectType": "library",
      "root": "projects/awd-ng-lib/session",
      "sourceRoot": "projects/awd-ng-lib/session/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/awd-ng-lib/session/tsconfig.lib.json",
            "project": "projects/awd-ng-lib/session/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/awd-ng-lib/session/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/awd-ng-lib/session/src/test.ts",
            "tsConfig": "projects/awd-ng-lib/session/tsconfig.spec.json",
            "karmaConfig": "projects/awd-ng-lib/session/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/awd-ng-lib/session/tsconfig.lib.json",
              "projects/awd-ng-lib/session/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
