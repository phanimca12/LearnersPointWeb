    "@awd-ng-lib/util": {
      "projectType": "library",
      "root": "projects/awd-ng-lib/util",
      "sourceRoot": "projects/awd-ng-lib/util/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
